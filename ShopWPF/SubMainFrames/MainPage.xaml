<Page x:Class="ShopWPF.SubMainFrames.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ShopWPF.SubMainFrames"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MainPage" Background="LightGray">

    <Grid>
        <DockPanel>
            <Label VerticalAlignment="Top" HorizontalAlignment="Center" FontFamily="Helvetica" FontWeight="Bold" FontSize="24" DockPanel.Dock="Top">SHOPS:
            </Label>
            <ListBox Name="ShopsListBox"  Margin="0,24,0,0" DockPanel.Dock="Top"></ListBox>
            <!--<Button Height="50" Width="200" DockPanel.Dock="Top">Back</Button>-->
        </DockPanel>
    </Grid>
</Page>
